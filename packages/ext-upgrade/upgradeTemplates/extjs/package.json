{
  "scripts": {
    "start": "cross-env-shell npm run dev",
    "clean": "rimraf build",
    "dev": "webpack-dev-server --env.browser='yes' --env.verbose='no'",
    "desktop": "webpack-dev-server --env.profile='desktop'",
    "phone": "webpack-dev-server --env.profile='phone'",
    "build": "npm run clean && cross-env webpack --env.environment='production' --env.treeshake='yes'",

    "build-desktop-testing": "npx sencha app build desktop testing",
    "build-desktop-production": "npx sencha app build desktop production",
    "build-phone-testing": "npx sencha app build phone testing",
    "build-phone-production": "npx sencha app build phone production",
    "createview": "cross-env-shell npx sencha generate viewpackage $npm_package_extbuild_defaultprofile $VIEW"
  },
  "dependencies": {
    "@sencha/ext-modern": "~6.7.0",
    "@sencha/ext-modern-theme-material": "~6.7.0",
    "@sencha/ext-modern-theme-ios": "~6.7.0",
    "@sencha/ext-font-ios": "~6.7.0",
    "@sencha/ext": "~6.7.0"
  },
  "devDependencies": {
    "@sencha/ext-webpack-plugin": "~6.7.1",
    "html-webpack-plugin": "^3.2.0",
    "base-href-webpack-plugin": "~2.0.0",
    "cross-env": "^5.2.0",
    "portfinder": "^1.0.20",

    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
}
